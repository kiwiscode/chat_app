generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id          Int           @id @default(autoincrement())
  email       String        @unique
  username    String        @unique
  password    String
  active      Boolean       @default(false)
  Preferences Preferences[]
}

model Preferences {
  id     Int   @id @default(autoincrement())
  userId Int
  user   Users @relation(fields: [userId], references: [id])
}
